# 81e1da018eef416f87e86db9260b6a15
msgid "Chunk Migration will fail:"
msgstr ""

#: ../source/release-notes/2.6-compatibility.txt:65
# ca007bad300d4cb5bc024df16c49b403
msgid "Migrations will fail for a chunk that has a document with an indexed field that exceeds the index key length limit."
msgstr ""

